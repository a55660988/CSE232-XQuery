    <zu>
        <titel>
            <test1>kk</test1>
            <tex>Dear 1</tex>
        </titel>
        <titel>
            <test2>kk</test2>
            <tex>Dear 1</tex>
        </titel>
        <titel>
            <tex>Dear 2....;-).</tex>
        </titel>
        <titel>
            <tex>Dear 3</tex>
        </titel>
    </zu>


    <prior type="schnell"/>
</email>

